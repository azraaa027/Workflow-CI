name: titanic-skilled

conda_env: conda.yaml

entry_points:
  train:
    parameters:
      dataset:
        type: string
        default: "titanic_preprocessed.csv"
      C:
        type: float
        default: 1.0
      max_iter:
        type: int
        default: 1000
    command: >
      python modelling.py
      --dataset {dataset}
      --C {C}
      --max_iter {max_iter}